<div class="container">
        <nav>
            <ul class="nav nav-tabs">
                <li role="presentation" class="active"><a href="#">Survey</a></li>
                <li role="presentation"><a href="/dashboard">Dashboard</a></li>
                <li role="presentation"><a href="#">Messages</a></li>
            </ul>

        </nav>
    </div>
    <br>
    <br>
    <div class="container">
       <div class="row">
  <div class="col-md-6"><h4>Building: {{building}}</h4></div>


<div class="col-md-2"><h4>Floor: {{floor_number}}</h4></div>


  <div class="col-md-4">
  <form class="form-inline">
  <div class="form-group ">
    <label for="goFloor"><h4>Go to floor:</h4></label>
    <input type="text" class="form-control input-mini" id="goFloor" placeholder="#">
  </div>
  </form>
  </div>

            <div class="panel panel-default">
                <div class="panel-body">
                    <table class="table table-striped">
                        <thead>
                            <tr class="headers">
                                <th class="col-xs-3">Room</th>
                                <th class="col-xs-5">Fixture</th>
                                <th class="col-xs-4"># of Fixtures</th>
                            </tr>
                        </thead>
                        <tbody>
                          {{#each surveys}}
                              <tr>
                                <td>{{this.room}}</td>
                                <td>{{this.PrefixturePreFixID}}</td>
                                <td>{{this.quantity}}</td>
                              </tr>

                          {{/each}}
                        </tbody>
                    </table>
                  </div>
                </div>
                    <div class="panel panel-default">
                     <div class="panel-body">
                        <form id="roomLine" class="form-inline">
                            <div class="form-group">
                                <input type="text" class="form-control" id="room" placeholder="room" required>
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control" id="fixture" placeholder="fixture" required>
                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control fixture" name-"fixture" id="num_fixture" placeholder="# of fixtures" required>
                            </div>
                            <button type="submit" class="btn btn-primary" id="submit"><i class="fa fa-user-plus" aria-hidden="true"></i>Add row</button>
                        </form>
                    </div>
                    </div>
                     <div class="panel panel-default">
                     <button type="submit" class="btn btn-success btn-lg" id="nextFloor"><i class="fa fa-user-plus" aria-hidden="true"></i>Go to next Floor</button>

                     </div>

                </div>

                

